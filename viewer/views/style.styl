// general
body
  margin: 0 auto

.bold
  font-weight: bold

.ulist-nav li
  display:inline
  font-size:1em
  border-right:1px solid black
  margin-right:1em
  padding-right:1em

.ulist-nav li.last
  border-right:none

.ulist-nav li a
  text-decoration:none
  color:white

.ulist-nav li a:hover
  text-decoration:underline

.ulist-side li
  list-style:none
  font-size:1em
  margin-right:1em
  padding-right:1em

.ulist-side li a
  text-decoration:none
  color:black

.ulist-side li a:hover
  text-decoration:underline

.fixedsearch
  position: fixed
  top: 0px
  right: 0px
  left: 0px
  background-color: #0f0 ! important;
  background: #162758; /* Old browsers */
  background: -moz-linear-gradient(top,  #162758 0%, #445b9a 50%, #445b9a 100%) /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#162758), color-stop(50%,#445b9a), color-stop(100%,#445b9a)) /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top,  #162758 0%,#445b9a 50%,#445b9a 100%) /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top,  #162758 0%,#445b9a 50%,#445b9a 100%) /* Opera 11.10+ */
  background: -ms-linear-gradient(top,  #162758 0%,#445b9a 50%,#445b9a 100%) /* IE10+ */
  background: linear-gradient(to bottom,  #162758 0%,#445b9a 50%,#445b9a 100%) /* W3C */

.fixedheader
  position: fixed
  right: 0px
  left: 0px
  background-color: #0f0 ! important

.fixedrow
  position: fixed
  right: 0px
  left: 0px

